<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>main page</title>
</head>
<body>
    <h2>board</h2>
    <table>
        <tr>
            <th>no.</th>
            <th>title</th>
            <th>date</th>
            <th>writer</th>
        </tr>
        <% post.forEach((el)=>{ %>
            <tr>
                <td><%= el.id %></td>
                <td> <a href="/board/detail/<%= el.id %>"><%= el.title %></a> </td>
                <td><%= el.createdAt %></td>
                <td><%= el.writer %></td>
            </tr>
        <% }); %>
    </table>
    <a href="/board/add">add</a>
</body>
</html>