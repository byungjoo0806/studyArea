{
  "compilerOptions" : {
      "module": "CommonJS",
      "outDir": "./dist",
      "target": "ES6",
      "esModuleInterop": true,
      "strict": true,
      "baseUrl": "./src",
      "paths": {
          "@user/*": ["user/*"]
      }
  },
  
  "include": ["src/**/*"],
  "exclude": ["**/*.test.ts"] // **/*.test.ts - .test.ts 확장자가 붙은 파일은 모두 제외
}